{
    "tasks": [
        {
            "label": "C++: build active file",
            "type": "cppbuild",
            "command": "/usr/bin/cpp",
            "args": [
                "-I",
                "/usr/local/include",
                "-I",
                "/usr/local/include/eigen3",
                "-I",
                "${workspaceFolder}/include",
                "-I",
                "${workspaceFolder}/test",
                "-I",
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
